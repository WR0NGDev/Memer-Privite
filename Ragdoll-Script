local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
	Name = "MemerPrivite",
	LoadingTitle = "MemerPrivite",
	LoadingSubtitle = "made by ! WrongDev#5291",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = Memer_Privite, -- Create a custom folder for your hub/game
		FileName = "Pro Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "monke", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = fasle -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "lol",
		Subtitle = "Sui",
		Note = "If This Loads Put in -Hello-",
		FileName = "My Key",
		SaveKey = false,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Hello"
	}
})

local Tab = Window:CreateTab("Main", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Ragdoll")

local Button = Tab:CreateButton({
	Name = "Anti Ragdoll",
	Callback = function()
	
        game.Players.LocalPlayer.Character:WaitForChild("Local Ragdoll", math.huge).Disabled = true 

	end,
})

local Tab = Window:CreateTab("Games", 4483362458) -- Title, Image

local Button = Tab:CreateButton({
	Name = "Infinte Yeld",
	Callback = function()

        loadstring(game:HttpGet('https://pastebin.com/raw/reZDR7GQ'))()

	end,
})

local Button = Tab:CreateButton({
	Name = "UniversalFE",
	Callback = function()

        loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalFEScriptHub/main/UFE'))()
	end,
